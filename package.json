{
  "name": "neonestate",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "emulator": "tsc && vite build --mode \"development\" && firebase emulators:start",
    "lint": "npm run lint:css && npm run lint:ts",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:ts": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fix": "npm run fix:css && npm run fix:ts",
    "fix:css": "stylelint --fix \"src/**/*.css\"",
    "fix:ts": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "firebase": "^10.9.0",
    "phone": "^3.1.42",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@testing-library/react": "^15.0.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}

{
  "name": "neonestate",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "deploy": "tsc && vite build --mode \"production\" && firebase deploy",
    "dev": "vite",
    "emulator": "tsc && vite build --mode \"development\" && firebase emulators:start",
    "fix": "npm run fix:css && npm run fix:ts && npm run fix:prettier",
    "fix:prettier": "npx prettier . --write",
    "fix:css": "stylelint --fix \"src/**/*.css\"",
    "fix:ts": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint": "npm run lint:css && npm run lint:ts && npm run lint:prettier",
    "lint:prettier": "npx prettier . --check",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:ts": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "@ctrl/react-adsense": "^1.7.0",
    "firebase": "^10.9.0",
    "phone": "^3.1.42",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "global-jsdom": "^24.0.0",
    "jsdom": "^24.0.0",
    "prettier": "3.3.2",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}
